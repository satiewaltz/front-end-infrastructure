{
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module"
    },
    "env": {
        "node": true,
        "browser": true,
        "es6": true
    },
    "rules": {
        "no-undef": 2,
        "no-var": 2,
        "semi": 2,
        "camelcase": 2,
        "eqeqeq": 2,
        "no-extra-semi": 2
    }
}
